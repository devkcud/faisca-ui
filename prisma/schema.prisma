generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Article {
  id            Int      @id @default(autoincrement())
  title         String
  summary       String
  body          String
  author        String[]
  advisor       String[]
  coordinator   String[]
  registered_at DateTime @default(now())
  updated_at    DateTime @updatedAt
  references    String[]
  pdf_url       String
  thumb_url     String
  inner_images  String[]
}
